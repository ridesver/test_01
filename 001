# test_01
#include <iostream>
#include <cstring>


using namespace std;

int main() 
{ 
    int i=80;
    char k[i];
    cin.getline (k, i);
    for (int i = 0; i <= (strlen(k)); i++) 
    {
        if (k[i] == ' ')
        
            cout << "%20";
    
        else
        cout << k[i];
    }
    return 0;
}
